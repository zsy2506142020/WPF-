<Window x:Class="algorithm_implement.TopologicalSortWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:algorithm_implement"
        mc:Ignorable="d"
        Title="TopologicalSortWindow" Height="620" Width="800">
    <Grid x:Name="ToplogicalSortGrid" Background="LightBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50" />
            <ColumnDefinition />
            <ColumnDefinition Width="50" />
        </Grid.ColumnDefinitions>
        <Label Content="拓扑排序" Grid.Column="1" Grid.Row="0" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center" Height="48" Margin="232,2,231.333,1.333" Width="130" />
        <ListBox x:Name="topologicalSortImages" Grid.Column="1" Grid.Row="1" ></ListBox>
    </Grid>
</Window>
